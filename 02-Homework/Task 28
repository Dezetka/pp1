height = int(input('Enter you height in cm: '))
weight = int(input('Enter your weight in kg: '))
height = height / 100
bmi = weight / height **2
print(bmi)
if bmi >= 18.5 and bmi <= 24.9:
    print('Correct weight:', bmi >= 18.5 and bmi <= 24.9)
elif bmi < 18.5 or bmi > 24.9:
    print('Correct weight:', bmi < 18.5 and bmi > 24.9)
          