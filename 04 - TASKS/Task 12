max_number = max(7, 5, 6, 3, 8, 2)
print('Max number of 7,5,6,3,8,2 is',max_number)

binary = bin(304)
print('The binary representation of 3004 is', binary)

hexa = hex(304)
print("The hexadecimal representation of 304 is ",hexa)

unicode = ord('€')
print("The unicdoe representation of € is",unicode)

absolute = abs(-17)  
print('The absolute value of -17 is',absolute)